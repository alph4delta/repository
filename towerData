      jQuery(document).ready(function(){
        var $ = jQuery

        $('#header .site-title').click(function(){
          window.location = "https://www.towerdata.com/";
        })

        //PRICING
        if($('body').is('.page-id-1163')){

          $('.page-id-1163 .entry .faq li > a').not('.more a').click(function(){
            $(this).parents('ul').find('.opened').not($(this).parent()).removeClass('opened')
            $(this).parent().toggleClass('opened');
            return false;
          })

          $('tr.field td:not(.section)').hover(function(){
            var tr=$(this).parents('tr');
            field=tr.find('input').attr('id').replace('field_','');
            $('.field_content').hide()
            $('.field_content.'+field).show();
            //$('.field_content.'+field).find('h5:first').text(tr.find('.checkbox').text())
            $('#field_tip').css('top',(tr.position().top-40)).show()
            if(tr.is(':not(.selected)')) tr.find('td').addClass('hover')
          },function(){
            var tr=$(this).parents('tr');
            $('#field_tip').hide()
            if(tr.is(':not(.selected)')) tr.find('td').removeClass('hover')
          });

          $('tr.field td:not(.section)').click(function(){
            var tr=$(this).parents('tr');
            tr.find('td').removeClass('hover')
            tr.toggleClass('selected')
            if(tr.is('.selected')){
              tr.find('input').attr('checked','true')
              calculatePrice()
            }else{
              tr.find('input').removeAttr('checked')
              calculatePrice()
            }
          })
          $('.select-all-th').click(function(e){
            var th=$('.select-all-th')
            var check=th.find('input')
            if(th.is('.checked')){
                $('tr.field').removeClass('selected').find('input').removeAttr('checked')
                th.removeClass('checked')
                if(check.attr('checked')) check.removeAttr('checked')
            }else{
                $('tr.field').addClass('selected').find('input').attr('checked','true')
                th.addClass('checked')
                if(!check.attr('checked')) check.attr('checked',true)
            }
            calculatePrice()
          });

          $('.sumcol_email input').autoNumeric({vMax: '500000000', mDec: '0', wEmpty: 'zero'}).keyup(function(e){
            /*
            // Allow: backspace, delete, tab, escape, and enter
            if ( event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 27 || event.keyCode == 13 ||
                 // Allow: Ctrl+A
                (event.keyCode == 65 && event.ctrlKey === true) ||
                 // Allow: home, end, left, right
                (event.keyCode >= 35 && event.keyCode <= 39)) {
                     // let it happen, don't do anything
                     return;
            }
            else {
                // Ensure that it is a number and stop the keypress
                if (event.shiftKey || (event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105 )) {
                    event.preventDefault();
                }
            }
            var num_emails=parseInt($('.sumcol_email input').val().replace(/\D/g,''))
            if(num_emails>=500000000) $('.sumcol_email input').val('500,000,000')
            //$('.sumcol_email input').val($('.sumcol_email input').val().replace(/\D/g,'').replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,"))
            $('.sumcol_email input').val($('.sumcol_email input').val().replace(/\D/g,'').replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,"))
            */
            calculatePrice()

          })
          $(window).scroll(function(){
            var summaryTop = 442;//var summaryTop=254;//=$('.summary').offset().top
            if($(window).scrollTop()>summaryTop){
              $('.summary').css({position:'fixed',top:6,height:'103px'})
            }else{
              $('.summary').css({position:'absolute',top:'auto',height:'134px'})
            }
            if($(window).scrollTop()>summaryTop && $(window).scrollTop()<=1200){
              $('.faq').css({position:'fixed',top:-45}).show()
            }else if($(window).scrollTop()>1200){
              $('.faq').hide()
            }else{
              $('.faq').css({position:'absolute',top:'auto'}).show()
            }
          })
          //add label for min and discount values
          $('.sumcol_total .sum_name').append(' <span class="label"></span>');
          //
          loadCookie()
        }
      })
      function calculatePrice(){
        var $ = jQuery
        var num_emails=parseInt($('.sumcol_email input').val().replace(/\D/g,''))
        $('td.checkbox:not(.select-all-th)').each(function(i,n){
          el=$(n)
          var rate=parseFloat(el.attr('rel'));
          var price = 0.01;
	  //var price=(el.is($('tr').eq(1).find('.checkbox')) || el.is($('tr').eq(2).find('.checkbox'))) ? .005 : .01;
          if(num_emails){
            el.next().text(Math.round(rate*num_emails).toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,"));
            if(rate*num_emails<1000000) el.next().next().next().text('$' + (rate*num_emails*price).toFixed(2).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,"));
            else  el.next().next().next().text('$' + Math.round((rate*num_emails*price)).toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,"));
          }else{
            el.next().text('â');
            el.next().next().next().text('$0.00');
          }
        })
        var field_count=$('tr.selected').length;
        $('.field_count').text(field_count)
        if(field_count>2){
          $('.first_field').text($('tr.selected:first .checkbox').text())
          $('.and_x_more').text('and '+(field_count-1)+' more')
        }else if(field_count==2){
          $('.first_field').text($('tr.selected:first .checkbox').text())
          $('.and_x_more').text('and '+$('tr.selected:eq(1) .checkbox').text())
        }else if(field_count==1){
          $('.first_field').text($('tr.selected:first .checkbox').text())
          $('.and_x_more').text('')
        }else{
          $('.first_field').text('')
          $('.and_x_more').text('')
        }
        var total_cost=0;
        $('tr.selected .price').each(function(i,n){
          total_cost+=parseFloat($(n).text().replace(/[^0-9|\.]/g,''))
        });

        $('.total_cost').next('p').text("")

        if(total_cost<=50){
          $('.sumcol_total .label').text('minimum').removeClass('discount').addClass('min');
          $('.total_cost').text('$50');
          $('.total_cost').next('p').text("It costs $ to make magic happen")
        }else{
          if(total_cost<20000){
            $('.sumcol_total .label').text('').removeClass('min discount')
          }else if(total_cost<110000){
            var discount_factor=(Math.floor(total_cost/10000)*5/100)
            total_cost=total_cost-total_cost*discount_factor
            $('.sumcol_total .label').removeClass('min').addClass('discount')
            $('.sumcol_total .label').text(Math.floor(discount_factor*100)+'% off')
          }else if(total_cost<600000){
            if(total_cost<200000){
              discount_factor=.5
            }else if(total_cost<300000){
              discount_factor=.55
            }else if(total_cost<400000){
              discount_factor=.6
            }else if(total_cost<500000){
              discount_factor=.65
            }else if(total_cost<600000){
              discount_factor=.7
            }else{
              discount_factor=.75
            }
            total_cost=total_cost-total_cost*discount_factor
            $('.sumcol_total .label').removeClass('min').addClass('discount')
            $('.sumcol_total .label').text(Math.floor(discount_factor*100)+'% off')
          }else{
            total_cost=total_cost-total_cost*.75
            $('.sumcol_total .label').removeClass('min').addClass('discount')
            $('.sumcol_total .label').text('75% off')
          }
          if(total_cost<1000000){
            $('.total_cost').text('$'+total_cost.toFixed(2).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,"))
          }else{
            $('.total_cost').text('$'+Math.round(total_cost).toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,"))
          }


        }

        writeCookie()
      }
