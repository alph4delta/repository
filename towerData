      // Load the selected fields from their cookie
        function loadCookie() {
          var selected_fields = jQuery.cookie('selected_fields') || ''
          selectedFields = selected_fields.split(',')
          if(selected_fields!=''){
            for (var i = 0; i < selectedFields.length; i++) {
              jQuery("#"+field_ids[selectedFields[i]]).parents('tr').addClass('selected').find('input').attr('checked','true')
            }
            //all have been selected
            if(jQuery('.field input').not(':checked').length<1){
              jQuery('.select-all-th').addClass('checked')
              jQuery('.select-all-th input').attr('checked',true)
            }

          }else{
            jQuery('#field_age, #field_gender, #field_zip').parent().click()
          }
          calculatePrice()

        }
        // Write their selected fields to the cookie
        function writeCookie() {
          var fields = []
          jQuery('input[type=checkbox]:checked').each(function(i,n) {
            fields.push(field_keys[jQuery(n).attr('id')])
          })
          jQuery.cookie('selected_fields', '' + fields, { expires: 30, domain: '.rapleaf.com', path: '/'})
        }
        /*!
         * jQuery Cookie Plugin
         * https://github.com/carhartl/jquery-cookie
         *
         * Copyright 2011, Klaus Hartl
         * Dual licensed under the MIT or GPL Version 2 licenses.
         * http://www.opensource.org/licenses/mit-license.php
         * http://www.opensource.org/licenses/GPL-2.0
         */
        (function(jQuery) {
            var $=jQuery;
            $.cookie = function(key, value, options) {

                // key and at least value given, set cookie...
                if (arguments.length > 1 && (!/Object/.test(Object.prototype.toString.call(value)) || value === null || value === undefined)) {
                    options = $.extend({}, options);

                    if (value === null || value === undefined) {
                        options.expires = -1;
                    }

                    if (typeof options.expires === 'number') {
                        var days = options.expires, t = options.expires = new Date();
                        t.setDate(t.getDate() + days);
                    }

                    value = String(value);

                    return (document.cookie = [
                        encodeURIComponent(key), '=', options.raw ? value : encodeURIComponent(value),
                        options.expires ? '; expires=' + options.expires.toUTCString() : '', // use expires attribute, max-age is not supported by IE
                        options.path    ? '; path=' + options.path : '',
                        options.domain  ? '; domain=' + options.domain : '',
                        options.secure  ? '; secure' : ''
                    ].join(''));
                }

                // key and possibly options given, get cookie...
                options = value || {};
                var decode = options.raw ? function(s) { return s; } : decodeURIComponent;

                var pairs = document.cookie.split('; ');
                for (var i = 0, pair; pair = pairs[i] && pairs[i].split('='); i++) {
                    if (decode(pair[0]) === key) return decode(pair[1] || ''); // IE saves cookies with empty string as "c; ", e.g. without "=" as opposed to EOMB, thus pair[1] may be undefined
                }
                return null;
            };
        })(jQuery);
    </script>
