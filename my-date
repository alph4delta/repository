}


			$("#modall")
  
  .slideDown('slow')
  .animate(
    { opacity: 1 },
    { queue: false }
  );
});

function dismiss() {
$("#modall")
  
  .slideUp('slow')
  .animate(
    { opacity: 0 },
    { queue: false, duration: 'slow' }
  );

$("#big").animate(
    { opacity: 0 },
    { queue: false, duration: 'slow' }
);

setTimeout(function(){
$( "#big" ).remove();
$( "#modall" ).remove();
}, 700);

}
